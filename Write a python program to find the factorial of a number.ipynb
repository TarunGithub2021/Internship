   "source": [
    "def factorial(n):\n",
    "    if n==0:\n",
    "        result=1\n",
    "    else:\n",
    "        result=n*factorial(n-1)\n",
    "    return result\n",
    "print('Factorial of 4 is:',factorial(4))\n",
    "print('Factorial of 5 is:',factorial(5))"
   ]
